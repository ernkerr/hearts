{
  "cli": {
    "version": ">= 16.1.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.11.0"
    },
    "preview": {
      "distribution": "internal",
      "node": "22.11.0"
    },
    "production": {
      "autoIncrement": true,
      "node": "22.11.0",
      "ios": {
        "cache": {
          "clear": true
        }
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "hello.cybergoose@gmail.com",
        "ascAppId": "6746460027",
        "appleTeamId": "J6RC5ZNC73"
      },
      "android": {
        "serviceAccountKeyPath": "../path/to/api-key.json",
        "track": "internal"
      }
    }
  }
}
