{
  "expo": {
    "name": "Hearts Score Tracker",
    "slug": "hearts-score-tracker",
    "scheme": "hearts-score-tracker",
    "version": "1.0.3",
    "splash": {
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.cybergoose.heartsscoretracker",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "org.cybergoose.heartsscoretracker",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      }
    },
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "bd649b89-aa5d-4474-97b5-e58939f92b28"
      }
    },
    "plugins": [
      "expo-font",
      [
        "react-native-iap",
        {
          "ios": {
            "enableStoreKit2": true
          },
          "android": {
            "store": "play"
          }
        }
      ]
    ],
    "icon": "./assets/images/icon.png"
  }
}
